{
  "build" : {
    "-D"    : ["no-deprecation-warnings"],
    "-cp"  : "src/main/haxe",
    "-lib" : ["stx_pico","stx_nano","stx_log","pml","stx_ds","stx_show"],
    "unit" : {
      "--macro" : [
        "include('stx.lense',true)"
      ],
      "interp" : {
        "--interp" : true
      }
    },
    "test" : {
      "--resource"  : [
        "asset/dataI.edn@dataI",
        "asset/rename.edn@rename"
      ],
      "-lib"        : ["stx_test"],
      "-cp"         : "src/test/haxe",
      "-main"       : "stx.lense.Test",
      "interp" : {
        "--interp" : true
      }
    }
  } 
}